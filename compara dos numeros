 }
 void max(int a,int b){
 	if(a>b||a<b){
	 if (a>b){
 		cout<<"El mayor es:"<<a<<endl;
	 }
	if(a<b){
		cout<<"el mayor es:"<<b<<endl;
	}
    }
	else{
		cout<<"los numeros son iguales"<<endl;
	}
}
