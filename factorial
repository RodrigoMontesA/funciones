/*factorial*/
#include <iostream>
#include <stdio.h>
#include <stdlib.h>
using namespace std;
int Factorial(int a);
int main(){
 	int num;
 	cout <<"introduzca  numero:"<<endl;
 	       cin>>num;
 	int fact=Factorial(num);
	cout<<"el factorial es: "<<fact<<endl;
	 return 1;       
}
int Factorial(int a){
	int i,resultado=1;
	for(i=1;i<=a;i++){
		resultado *=i;
	}
	return resultado;
}
